<div class="container w-medium mt-8">
  <details class="collapse bg-base-200">
    <summary class="collapse-title text-xl font-medium">Create new post</summary>
    <div class="collapse-content">
      <%= render(FeedPostFormComponent.new) %>
    </div>
  </details>

  <% @feed_posts.each do |post| %>
    <%= render(FeedPostComponent.new(title: post.title, body: post.body, updated_at: post.updated_at, post_id: post.id)) %>
  <% end %>
</div>
